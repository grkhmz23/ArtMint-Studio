{
  "name": "artmint-studio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @artmint/web dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "db:migrate": "pnpm --filter @artmint/web prisma:migrate",
    "db:migrate:deploy": "prisma migrate deploy --schema=prisma/schema.prisma",
    "db:studio": "pnpm --filter @artmint/web prisma:studio",
    "vercel:build": "node scripts/vercel-build.js",
    "lint": "pnpm -r lint",
    "postinstall": "prisma generate --schema=prisma/schema.prisma"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "@resvg/resvg-js",
      "esbuild"
    ],
    "overrides": {
      "lodash": "^4.17.23",
      "minimatch": "^10.2.1"
    }
  },
  "devDependencies": {
    "prisma": "5.22.0"
  },
  "dependencies": {
    "@prisma/client": "5.22.0"
  }
}
